<%= form_for @goal do |f| %>

<%= f.label :title %>
<%= f.text_field :title %> <br />

<%= f.label :deadline %>
<%= f.text_field :deadline %> <br />

<%= f.label :description %>
<%= f.text_field :description %> <br />

<%= f.label :goal_pic %>
<%= f.text_field :goal_pic %> <br />

<%= f.label :user_id %>
<%= f.text_field :user_id %> <br />


<%= f.submit %>
<% end %>