<%= form_for @goal, :url => user_goals_path do |f| %>

<%= f.label :title %>
<%= f.text_field :title %> <br />

<%= f.label :deadline %>
<%= f.text_field :deadline %> <br />

<%= f.label :description %>
<%= f.text_field :description %> <br />

<%= f.label :goal_pic %>
<%= f.text_field :goal_pic %> <br />



<%= f.submit %>
<% end %>