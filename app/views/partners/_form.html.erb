<%= form_for @partner do |f| %>

<%= f.label :first_name %>
<%= f.text_field :first_name %> <br />

<%= f.label :last_name %>
<%= f.text_field :last_name %> <br />

<%= f.label :email_address %>
<%= f.text_field :email_address %> <br />

<%= f.label :phone_number %>
<%= f.text_field :phone_number %> <br />

<%= f.label :goal_id %>
<%= f.text_field :goal_id %> <br />


<%= f.submit %>
<% end %>