<%= form_for @partner, :url => user_partner_path do |f| %>

<%= f.label :first_name %>
<%= f.text_field :first_name %> <br />

<%= f.label :last_name %>
<%= f.text_field :last_name %> <br />

<%= f.label :email_address %>
<%= f.text_field :email_address %> <br />

<%= f.label :phone_number %> *
<%= f.text_field :phone_number %><br />

<%= f.label :frequency %>
<%= f.text_field :frequency %> <br />

<%= f.label :goal_id %>
<%= select("partner", "goal_id", @goals.collect {|p| [ p.title, p.id ]}, {include_blank: 'None'}) %><br />

<%= f.submit %><br />
<p>* Required</p>
<% end %>

